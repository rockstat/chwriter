    "ejs": "^2.5.7",
    "glob": "^7.1.2",
    "js-yaml": "^3.11.0",
    "lazy.js": "^0.5.1",
    "merge-options": "^1.0.0",
    "node-fetch": "^2.1.2",
    "qs": "^6.5.1",
    "reflect-metadata": "^0.1.12",
    "rockmets": "1.0.0",
    "source-map-support": "^0.5.5",
    "xxhash": "^0.2.4"
  }
