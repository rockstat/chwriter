{
  "name": "rst-clickhouse-writer",
  "version": "0.0.1",
  "description": "Rockstat clickhouse writer service",
  "main": "dist/start.js",
  "scripts": {
    "start": "ts-node-dev -r 'tsconfig-paths/register' ./src/start.ts | pino",
    "build": "rimraf dist && tsc  -r 'source-map-support/register' -p tsconfig.json",
    "prod": "node dist/start",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json -t stylish -p ./tsconfig.json",
    "security-check": "nsp check",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rockstat/clickhouse_writer"
  },
  "author": "Dmitry Rodin <madiedinro@gmail.com>",
  "homepage": "https://rockstat.ru",
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/dotenv": "^4.0.2",
    "@types/ejs": "^2.5.0",
    "@types/glob": "^5.0.35",
    "@types/js-yaml": "^3.10.1",
    "@types/lazy.js": "^0.3.29",
    "@types/node": "^10.0.4",
    "@types/qs": "^6.5.1",
    "pino-pretty": "^1.0.1",
    "ts-node": "^6.0.3",
    "ts-node-dev": "^1.0.0-pre.21",
    "tsc-watch": "^1.0.17",
    "tsconfig-paths": "^3.3.2",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "ajv": "^6.4.0",
    "bluebird": "^3.5.1",
    "cctz": "^1.7.7",
